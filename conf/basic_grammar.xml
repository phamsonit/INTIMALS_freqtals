<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<node>
  <ClassBodyDeclaration>
    <body unordered-nodelist="BodyDeclaration"/>
  </ClassBodyDeclaration>

  <BodyDeclaration abstract="true">
    <FieldDeclaration/>
  </BodyDeclaration>
  <BodyDeclaration abstract="true">
    <MethodDeclaration/>
  </BodyDeclaration>

  <FieldDeclaration>
    <modifiers optional="true" ordered-nodelist="ModifierKeyword"/>
    <type optional="true" node="Type"/>
    <name simplevalue="String"/>
  </FieldDeclaration>
  <MethodDeclaration>
    <modifiers ordered-nodelist="ModifierKeyword"/>
    <type node="Type"/>
    <name simplevalue="String"/>
    <body optional="true" unordered-nodelist="Expression"/>
  </MethodDeclaration>

  <ModifierKeyword abstract="true">
    <PUBLIC_KEYWORD image="public"/>
  </ModifierKeyword>
  <ModifierKeyword abstract="true">
    <STATIC_KEYWORD image="static"/>
  </ModifierKeyword>

  <Type abstract="true">
    <INTEGER_TYPE/>
  </Type>

  <Expression abstract="true">
    <InfixExpression/>
  </Expression>
  <Expression abstract="true">
    <NullLiteral/>
  </Expression>
  <InfixExpression>
    <left simplevalue="String"/>
    <op node="Operator"/>
    <right simplevalue="String"/>
  </InfixExpression>
  <NullLiteral>
    <token simplevalue="String"/>
  </NullLiteral>

  <Operator abstract="true">
    <PLUS/>
  </Operator>
</node>